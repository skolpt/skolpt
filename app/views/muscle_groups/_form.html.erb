<%= render 'shared/errors', obj: @muscle_group %>

<div class='row'>
  <div class='col-lg-6 col-lg-offset-3'>
    <%= simple_form_for @muscle_group do |f| %>
      <div class="form-group">
        <%= f.input :name %>
        <%= f.input :description, as: :text %>
      </div>
        
      <div class="form-group">
        <%= f.button :submit, class: "btn btn-success" %>
      </div>
    <% end %>
    
  </div>
</div>
